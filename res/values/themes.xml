<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <color name="paper_tanzil">#f7fce3</color>

    <style name="Black" parent="@style/Theme.Sherlock">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="pagerTitleStripBackground">#333333</item>
        <item name="pagerTitleStripTextColor">#fff</item>
        <item name="actionBarStyle">@style/Widget.Sherlock.ActionBar.Solid</item>
        <item name="android:actionBarStyle">@style/Widget.Sherlock.ActionBar.Solid</item>
    </style>

    <style name="White" parent="@style/Theme.Sherlock.Light.DarkActionBar">
        <item name="android:windowBackground">@android:color/white</item>
        <item name="pagerTitleStripBackground">#33b5e5</item>
        <item name="pagerTitleStripTextColor">#fff</item>
    </style>

    <style name="Paper" parent="@style/Theme.Sherlock.Light.DarkActionBar">
        <item name="android:windowBackground">@color/paper_tanzil</item>
        <item name="pagerTitleStripBackground">#88b131</item>
        <item name="pagerTitleStripTextColor">#fff</item>
    </style>

</resources>